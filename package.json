{
  "name": "vue-material-design-icons",
  "version": "1.3.0",
  "author": "Rob Cresswell <robert.cresswell@outlook.com>",
  "license": "MIT",
  "description": "A collection of material design icons as Vue single file components",
  "files": "dist",
  "repository": "gitlab:robcresswell/vue-material-design-icons",
  "keywords": [
    "vue",
    "material design icons",
    "mdi",
    "icons",
    "material",
    "icon",
    "svg",
    "vue.js",
    "material design"
  ],
  "devDependencies": {
    "@mdi/svg": "2.3.54",
    "mustache": "2.3.0",
    "standard-version": "4.3.0"
  },
  "homepage": "https://gitlab.com/robcresswell/vue-material-design-icons",
  "bugs": {
    "url": "https://gitlab.com/robcresswell/vue-material-design-icons/issues"
  },
  "scripts": {
    "prebuild": "yarn",
    "build": "./build.js && cp styles.css dist/",
    "clean": "rm -rf dist",
    "prerelease": "yarn build",
    "release": "yarn standard-version && cp package.json README.md CHANGELOG.md dist/ && cd dist && yarn publish"
  }
}
